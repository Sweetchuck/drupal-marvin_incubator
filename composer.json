{
    "name": "drupal/marvin_incubator",
    "description": "Drush commands to manage contrib development",
    "type": "drupal-drush",
    "license": "GPL-2.0-or-later",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "conflict": {
        "drupal/marvin_product": "*"
    },
    "repositories": {
        "drupal/dummy_m1": {
            "type": "path",
            "url": "tests/fixtures/extensions/dummy_m1"
        },
        "drupal/dummy_m2": {
            "type": "path",
            "url": "tests/fixtures/extensions/dummy_m2"
        },
        "drupal/marvin": {
            "type": "path",
            "url": "../../drupal/marvin"
        },
        "sweetchuck/git-hooks-stdinput-reader": {
            "type": "path",
            "url": "../../sweetchuck/git-hooks-stdinput-reader"
        },
        "sweetchuck/lint-report": {
            "type": "path",
            "url": "../../sweetchuck/lint-report"
        },
        "sweetchuck/robo-bundler": {
            "type": "path",
            "url": "../../sweetchuck/robo-bundler"
        },
        "sweetchuck/robo-cdd": {
            "type": "path",
            "url": "../../sweetchuck/robo-cdd"
        },
        "sweetchuck/robo-compass": {
            "type": "path",
            "url": "../../sweetchuck/robo-compass"
        },
        "sweetchuck/robo-composer": {
            "type": "path",
            "url": "../../sweetchuck/robo-composer"
        },
        "sweetchuck/robo-drush": {
            "type": "path",
            "url": "../../sweetchuck/robo-drush"
        },
        "sweetchuck/robo-git": {
            "type": "path",
            "url": "../../sweetchuck/robo-git"
        },
        "sweetchuck/robo-nvm": {
            "type": "path",
            "url": "../../sweetchuck/robo-nvm"
        },
        "sweetchuck/robo-phpcs": {
            "type": "path",
            "url": "../../sweetchuck/robo-phpcs"
        },
        "sweetchuck/robo-phpmd": {
            "type": "path",
            "url": "../../sweetchuck/robo-phpmd"
        },
        "sweetchuck/robo-phpunit": {
            "type": "path",
            "url": "../../sweetchuck/robo-phpunit"
        },
        "sweetchuck/robo-rvm": {
            "type": "path",
            "url": "../../sweetchuck/robo-rvm"
        },
        "sweetchuck/robo-sass": {
            "type": "path",
            "url": "../../sweetchuck/robo-sass"
        },
        "sweetchuck/robo-scss-lint": {
            "type": "path",
            "url": "../../sweetchuck/robo-scss-lint"
        },
        "sweetchuck/robo-tslint": {
            "type": "path",
            "url": "../../sweetchuck/robo-tslint"
        },
        "sweetchuck/robo-yarn": {
            "type": "path",
            "url": "../../sweetchuck/robo-yarn"
        },
        "sweetchuck/utils": {
            "type": "path",
            "url": "../../sweetchuck/utils"
        }
    },
    "require": {
        "ext-dom": "*",
        "ext-json": "*",
        "drupal/marvin": "dev-8.x-1.x",
        "drush/drush": "^9.3",
        "sweetchuck/robo-php-lint": "^0.0",
        "sweetchuck/robo-stringy": "^0.0"
    },
    "require-dev": {
        "composer/installers": "^1.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/coder": "^8.2",
        "drupal/core": "^8.6",
        "drupal/dummy_m1": "dev-8.x-1.x",
        "drupal/dummy_m2": "dev-8.x-1.x",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^6.3",
        "sweetchuck/git-hooks": "^0.0"
    },
    "autoload": {
        "psr-4": {
            "Drush\\Commands\\marvin_incubator\\": "Commands/marvin_incubator/",
            "Drupal\\marvin_incubator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Unish": "vendor/drush/drush/tests/"
        },
        "psr-4": {
            "Drush\\Commands\\Tests\\marvin_incubator\\Unish\\": "tests/src/DrushUnish/",
            "Drush\\Commands\\Tests\\marvin_incubator\\Unit\\": "tests/src/DrushUnit/",
            "Drupal\\Tests\\marvin\\Unit_incubator\\": "tests/src/Unit/",
            "Drupal\\Dev\\marvin\\Composer\\": "tests/fixtures/drush-sut/drush/unish/marvin/src-dev/Composer/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "8.x-1.x"
        },
        "drush": {
            "services": {
                "drush9.services.yml": "^9"
            }
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "excludes": [
                ".csslintrc",
                ".editorconfig",
                ".eslintignore",
                ".eslintrc.json",
                ".gitattributes",
                ".ht.router.php",
                ".htaccess",
                "web.config"
            ]
        },
        "installer-paths": {
            "tests/fixtures/drush-sut/web/core": ["type:drupal-core"],
            "tests/fixtures/drush-sut/web/libraries/{$name}": ["type:drupal-library"],
            "tests/fixtures/drush-sut/web/modules/unish/{$name}": ["type:drupal-module"],
            "tests/fixtures/drush-sut/web/profiles/unish/{$name}": ["type:drupal-profile"],
            "tests/fixtures/drush-sut/web/themes/unish/{$name}": ["type:drupal-theme"],
            "tests/fixtures/drush-sut/drush/unish/{$name}": ["type:drupal-drush"]
        },
        "marvin": {
            "drushUnish": {
                "filesToSymlink": {
                    "gitHooks": true
                }
            }
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "\\Drupal\\Dev\\marvin\\Composer\\Scripts::preInstallCmd"
        ],
        "post-install-cmd": [
            "\\Drupal\\Dev\\marvin\\Composer\\Scripts::postInstallCmd"
        ],
        "pre-update-cmd": [
            "\\Drupal\\Dev\\marvin\\Composer\\Scripts::preUpdateCmd"
        ],
        "post-update-cmd": [
            "\\Drupal\\Dev\\marvin\\Composer\\Scripts::postUpdateCmd"
        ]
    }
}
